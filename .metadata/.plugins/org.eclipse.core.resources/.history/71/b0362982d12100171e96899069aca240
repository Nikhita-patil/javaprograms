package dataStructure;

public class BubbleSort {

	public static String[] bubbleSortInt(String string[]) {
		String temp_String = "";
		for (int t = 0; t < string.length - 1; t++) {
			for (int i = 0; i < string.length - t - 1; i++) {
				if (string[i + 1].compareTo(string[i]) < 0) {
					temp_String = string[i];
					string[i] = string[i + 1];
					t1[i + 1] = temp_String;
				}
			}
		}
		return t1;
	}

	public static void main(String[] args) {
		String string_array[] = { "Nikita", "Chaitra", "Miral", "Modi" };
		string_array = bubbleSortInt(string_array);
		for (String string_array1 : string_array)
			System.out.println(string_array1);
	}

}

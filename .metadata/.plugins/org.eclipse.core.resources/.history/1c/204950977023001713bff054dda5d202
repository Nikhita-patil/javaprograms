package com.bridgelabz.colletion_prgms;

public class BookArraylist{

		int id;  
		String name,author,publisher;  
		int quantity;  
		public public BookArraylist() {
			// TODO Auto-generated constructor (int id, String name, String author, String publisher, int quantity) {  
		    this.id = id;  
		    this.name = name;  
		    this.author = author;  
		    this.publisher = publisher;  
		    this.quantity = quantity;  
		}  

}
